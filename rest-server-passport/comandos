certificado
cd estudio\nodejsPractice\rest-server-passport\bin

set RANDFILE=D:\estudio\nodejsPractice\rest-server-passport\bin\.rnd
set OPENSSL_CONF=D:\Desarrollo\openssl-0.9.8k_X64\openssl.cnf
d:\Desarrollo\openssl-0.9.8k_X64\bin\openssl.exe
openssl genrsa 1024 > private.key
openssl req -new -key private.key -out cert.csr
openssl x509 -req -in cert.csr -signkey private.key -out certificate.pem


